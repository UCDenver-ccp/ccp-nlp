<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>TypeSystem</name>
  <description>This is a default Type System. It includes general types necessary to store annotations in the Annotation DB and interface with Protege</description>
  <version>1.0</version>
  <vendor>UCHSC - CCP</vendor>
  <imports>
    <import name="edu.ucdenver.ccp.nlp.core.uima.TypeSystem"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSyntacticAnnotation</name>
      <description>Super class for all syntactic annotations.</description>
      <supertypeName>edu.ucdenver.ccp.nlp.core.uima.annotation.CCPTextAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPTokenAnnotation</name>
      <description>This annotation type is used to mark up tokens.</description>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSyntacticAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPPhraseAnnotation</name>
      <description>This annotation type is used to annotation phrases.</description>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSyntacticAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPClauseAnnotation</name>
      <description>This annotation type is used to annotation clauses.</description>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSyntacticAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSentenceAnnotation</name>
      <description>This annotation type is used to annotate sentences within the document text.</description>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPSyntacticAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPTypedDependency</name>
      <description>This typed dependency structure was created to hold typed dependency information generated by the Stanford Parser.</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>governorTokenNum</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>dependentTokenNum</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>relation</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.TagSetLabel</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>label</name>
          <description>The part of speech label (tag).</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tagSet</name>
          <description>A String identifier for the tag set to which this tag belongs.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.SyntacticAnnotationProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.core.uima.annotation.metadata.AnnotationProperty</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.ClauseAnnotationProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.SyntacticAnnotationProperty</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.PhraseAnnotationProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.SyntacticAnnotationProperty</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.SyntacticAnnotationProperty</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.ClauseTypeProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.ClauseAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>clauseType</name>
          <description/>
          <rangeTypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.TagSetLabel</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.PhraseTypeProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.PhraseAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>phraseType</name>
          <description/>
          <rangeTypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.TagSetLabel</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.PartOfSpeechProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>partOfSpeech</name>
          <description/>
          <rangeTypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.TagSetLabel</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.StemProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>stem</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.LemmaProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>lemma</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TypedDependencyProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>typedDependency</name>
          <description/>
          <rangeTypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.CCPTypedDependency</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenNumberProperty</name>
      <description/>
      <supertypeName>edu.ucdenver.ccp.nlp.ext.uima.annotation.syntax.metadata.TokenAnnotationProperty</supertypeName>
      <features>
        <featureDescription>
          <name>tokenNumber</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
